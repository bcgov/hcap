{
  "name": "health-career-access-program-server",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "parse-xml:local": "cd scripts && node parse-xml.ts xml",
    "parse-xml:": "cd build/scripts && node parse-xml.js xml",
    "migrate:local": "npx ts-node node_modules/.bin/node-pg-migrate",
    "migrate": "npx node-pg-migrate -m build/migrations",
    "feed-participants": "npx ts-node scripts/feed-participants.ts",
    "feed-sites": "npx ts-node scripts/feed-sites.ts",
    "feed-data": "npx ts-node scripts/feed-data.ts",
    "clear-data": "npx ts-node scripts/clear-data.ts",
    "stats": "npx ts-node scripts/stats.ts",
    "sendMassEmail": "npx ts-node scripts/sendEmailBlast.ts",
    "participant-stats-hired": "npx ts-node scripts/participant-stats.ts hired",
    "participant-stats-in-progress": "npx ts-node scripts/participant-stats.ts in-progress",
    "participant-stats-rejected": "npx ts-node scripts/participant-stats.ts rejected",
    "participant-stats-no-offers": "npx ts-node scripts/participant-stats.ts no-offers",
    "clean-stale-cronjob": "node build/scripts/clean-up.js",
    "clean-stale-open-cronjob": "node build/scripts/withdraw-stale-open-participants.js",
    "archive-withdrawn-participants:local": "npx ts-node scripts/archive-withdrawn-participants.ts",
    "archive-withdrawn-participants": "node build/scripts/archive-withdrawn-participants.js",
    "generate-participants:local": "npx ts-node scripts/randomize-participants.ts",
    "generate-participants": "node build/scripts/randomize-participants.js",
    "export": "npx ts-node scripts/export.ts",
    "export:prod": "node build/scripts/export.js",
    "watch": "nodemon main.ts",
    "debug": "nodemon --exec 'node --inspect=0.0.0.0:9229 --require ts-node/register main.ts'",
    "start": "NODE_ENV=production ts-node -T main.ts",
    "start:prod": "NODE_ENV=production node build/main.js",
    "test:debug": "NODE_ENV=test node --inspect=0.0.0.0:9229 ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles",
    "test": "NODE_ENV=test jest --silent --colors --runInBand --forceExit --detectOpenHandles --coverage --verbose false"
  },
  "dependencies": {
    "@supercharge/promise-pool": "^3.2.0",
    "@types/jsdom": "^21.1.7",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fast-csv": "^5.0.1",
    "fast-xml-parser": "^4.5.0",
    "helmet": "^8.0.0",
    "inquirer": "^12.9.6",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.2",
    "keycloak-connect": "^26.1.1",
    "lodash": "^4.17.21",
    "massive": "^6.11.3",
    "minimist": "^1.2.8",
    "mongodb": "^6.20.0",
    "node-cache": "^5.1.2",
    "node-pg-migrate": "^7.7.1",
    "node-xlsx": "^0.24.0",
    "pg": "^8.13.1",
    "pg-promise": "^12.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "string-replace-middleware": "^1.0.2",
    "tiny-async-pool": "^2.1.0",
    "tmp": "^0.2.5",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-mongodb": "^6.0.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.3.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/massive": "^5.4.7",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 60000,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "./tests/util/extensions.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "preset": "ts-jest",
    "transform": {
      ".(js|ts)": "ts-jest"
    }
  }
}
