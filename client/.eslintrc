{
  "extends": ["react-app", "plugin:jsx-a11y/recommended"],
  "plugins": ["react", "react-hooks", "jsx-a11y"],
  "rules": {
    "import/no-anonymous-default-export": [2, { "allowArrowFunction": true }],
    "no-unused-vars": "error"
  },
  "settings": {
    "react": {
      "version": "16.13.1"
    }
  }
}
