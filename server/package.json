  "scripts": {
    "db:seed": "node db/scripts/seed.js",
    "db:migrate-up": "cd db && migrate up",
    "db:migrate-down": "cd db && migrate down",
    "db:migrate-create": "cd db && migrate create",
    "watch": "NODE_ENV=development nodemon main.js",
    "start": "NODE_ENV=production node main.js",
    "test": "NODE_ENV=test jest --runInBand"
  },
  "dependencies": {
    "axios": "0.20.0",
    "body-parser": "1.19.0",
    "crypto": "1.0.1",
    "express": "4.17.1",
    "migrate": "1.7.0",
    "mongodb": "3.6.0",
    "node-cache": "5.1.2",
    "winston": "3.3.3",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "husky": "4.2.5",
    "jest": "25.3.0",
    "lint-staged": "10.2.13",
    "nodemon": "2.0.4",
    "supertest": "4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
