{
  "scripts": {
    "format": "run-s 'format:*'",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "format:lint": "run-s 'lint:*'",
    "lint:client": "eslint --ext js,jsx -c ./client/.eslintrc ./client",
    "lint:server": "eslint --ext js,jsx -c ./server/.eslintrc ./server",
    "pretty-quick": "pretty-quick",
    "update-all": "run-s 'update:*'",
    "update:root": "npm i",
    "update:client": "npm i --prefix client",
    "update:server": "npm i --prefix server"
  },
  "devDependencies": {
    "cypress": "6.1.0",
    "cypress-dotenv": "1.2.2",
    "dotenv": "8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0"
  },
  "dependencies": {
    "uuidv4": "6.2.6"
  },
  "name": "hcap"
}
